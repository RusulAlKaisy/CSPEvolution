#Let us us figure out what sites of the categories belong to what region 

#Define the range for the C-terminal


#First replicate  with an outgroup. class model H5

class_model_H5_FirstDatasetWithOutgroup<-read.table("FirstDataSet_LocationswithOutGroupH5GHOST.fasta.siteprob",header=TRUE,row.names = 1)

cat_number<-1

C_terminal<-859:1233

site_categories_model_H5_FirstDataSetWithOutgroup<-apply(class_model_H5_FirstDatasetWithOutgroup, 1, function(x) which(x == max(x))) 

class_sites<-which(site_categories_model_H5_FirstDataSetWithOutgroup==cat_number)

proportion_in_C_terminal<-sum(class_sites%in% C_terminal)/length(class_sites)


#Second class 

cat_number<-2

site_categories_model_H5_FirstDataSetWithOutgroup<-apply(class_model_H5_FirstDatasetWithOutgroup, 1, function(x) which(x == max(x))) 

class_sites<-which(site_categories_model_H5_FirstDataSetWithOutgroup==cat_number)

proportion_in_C_terminal<-sum(class_sites%in% C_terminal)/length(class_sites)
                                                                                    

#Third class 
cat_number<-3

site_categories_model_H5_FirstDataSetWithOutgroup<-apply(class_model_H5_FirstDatasetWithOutgroup, 1, function(x) which(x == max(x))) 

class_sites<-which(site_categories_model_H5_FirstDataSetWithOutgroup==cat_number)

proportion_in_C_terminal<-sum(class_sites%in% C_terminal)/length(class_sites)

#Fourth class

cat_number<-4

site_categories_model_H5_FirstDataSetWithOutgroup<-apply(class_model_H5_FirstDatasetWithOutgroup, 1, function(x) which(x == max(x))) 

class_sites<-which(site_categories_model_H5_FirstDataSetWithOutgroup==cat_number)

proportion_in_C_terminal<-sum(class_sites%in% C_terminal)/length(class_sites)                                                                                                    

#Fifth class
cat_number<-5

site_categories_model_H5_FirstDataSetWithOutgroup<-apply(class_model_H5_FirstDatasetWithOutgroup, 1, function(x) which(x == max(x))) 

class_sites<-which(site_categories_model_H5_FirstDataSetWithOutgroup==cat_number)

proportion_in_C_terminal<-sum(class_sites%in% C_terminal)/length(class_sites)   

#First replicate without an outgroup 

