class_sites_First_Replicate_NOOutgroup_H5<-which(site_categories_model_H5_FirstDataSetNOOutgroup==cat_number_First_Replicate_H5_NOOutgroup)

proportion_in_C_terminal_First_Replicate_NOOutgroup_H5<-sum(class_sites_First_Replicate_NOOutgroup_H5%in%C_terminal)/length(class_sites_First_Replicate_NOOutgroup_H5)

