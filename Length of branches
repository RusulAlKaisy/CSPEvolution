Let us plot the phylogenetic trees and divide branch lengths 

#Phylogenetic tree - First replicate with an outgroup - H5 classmodel 

First_replicate_withOutgroup_Phylo_H5<-read.tree("FirstDataSet_LocationswithOutgroupH5GHOST.fasta.treefile")

tree1_FirstReplicate_Withoutgroup_H5_plot<-plot(First_replicate_withOutgroup_Phylo_H5,cex=0.05)

#Let's divide the branch lengths

tree1<-First_replicate_withOutgroup_Phylo_H5[[1]]

tree2<-First_replicate_withOutgroup_Phylo_H5[[2]]

tree3<-First_replicate_withOutgroup_Phylo_H5[[3]]

tree4<-First_replicate_withOutgroup_Phylo_H5[[4]]

tree5<-First_replicate_withOutgroup_Phylo_H5[[5]]

tree6<-First_replicate_withOutgroup_Phylo_H5[[6]]


#tree3<-tree1

tree2$edge.length<-tree2$edge.length/tree1$edge.length

plot(tree2,cex=0.05)

#tree4<-tree2

tree3$edge.length<-tree3$edge.length/tree1$edge.length
plot(tree3,cex=0.05)

#tree5<-tree3

tree4$edge.length<-tree4$edge.length/tree1$edge.length
plot(tree4,cex=0.05)

#tree6<-tree4

tree5$edge.length<-tree5$edge.length/tree1$edge.length
plot(tree5,cex=0.05)

#tree7<-tree6

tree6$edge.length<-tree6$edge.length/tree1$edge.length
plot(tree7,cex=0.05)

#First replicate with no outgroup. class model H5

First_replicate_NOOutgroup_Phylo_H5<-read.tree("FirstDataSet_LocationsNOOutgroupH5GHOST.fasta.treefile")

tree1_NOOutgroup_First<-First_replicate_NOOutgroup_Phylo_H5[[1]]

tree2_NOOutgroup_First<-First_replicate_NOOutgroup_Phylo_H5[[2]]

tree3_NOOutgroup_First<-First_replicate_NOOutgroup_Phylo_H5[[3]]

tree4_NOOutgroup_First<-First_replicate_NOOutgroup_Phylo_H5[[4]]

tree5_NOOutgroup_First<-First_replicate_NOOutgroup_Phylo_H5[[5]]

tree6_NOOutgroup_First<-First_replicate_NOOutgroup_Phylo_H5[[6]]

tree2_NOOutgroup_First$edge.length<-tree2_NOOutgroup_First$edge.length/tree1_NOOutgroup_First$edge.length
plot(tree2_NOOutgroup_First,cex=0.05)

tree3_NOOutgroup_First$edge.length<-tree3_NOOutgroup_First$edge.length/tree1_NOOutgroup_First$edge.length
plot(tree3_NOOutgroup_First,cex=0.05)

tree4_NOOutgroup_First$edge.length<-tree4_NOOutgroup_First$edge.length/tree1_NOOutgroup_First$edge.length
plot(tree4_NOOutgroup_First,cex=0.05)

tree5_NOOutgroup_First$edge.length<-tree5_NOOutgroup_First$edge.length/tree1_NOOutgroup_First$edge.length
plot(tree5_NOOutgroup_First,cex=0.05)

tree6_NOOutgroup_First$edge.length<-tree6_NOOutgroup_First$edge.length/tree1_NOOutgroup_First$edge.length
plot(tree6_NOOutgroup_First,cex=0.05)

#Second replicate with outgroup.class model H5 
Second_replicate_withOutgroup_Phylo_H5<-read.tree("SecondDataSet_LocationswithOutgroupH5GHOST.fasta.treefile")

tree1_withOutgroup_Second<-Second_replicate_withOutgroup_Phylo_H5[[1]]

tree2_withOutgroup_Second<-Second_replicate_withOutgroup_Phylo_H5[[2]]

tree3_withOutgroup_Second<-Second_replicate_withOutgroup_Phylo_H5[[3]]

tree4_withOutgroup_Second<-Second_replicate_withOutgroup_Phylo_H5[[4]]

tree5_withOutgroup_Second<-Second_replicate_withOutgroup_Phylo_H5[[5]]

tree6_withOutgroup_Second<-Second_replicate_withOutgroup_Phylo_H5[[6]]


tree2_withOutgroup_Second$edge.length<-tree2_withOutgroup_Second$edge.length/tree1_withOutgroup_Second$edge.length
plot(tree2_withOutgroup_Second,cex=0.05)

tree3_withOutgroup_Second$edge.length<-tree3_withOutgroup_Second$edge.length/tree1_withOutgroup_Second$edge.length
plot(tree3_withOutgroup_Second,cex=0.05)

tree4_withOutgroup_Second$edge.length<-tree4_withOutgroup_Second$edge.length/tree1_withOutgroup_Second$edge.length
plot(tree4_withOutgroup_Second,cex=0.05)

tree5_withOutgroup_Second$edge.length<-tree5_withOutgroup_Second$edge.length/tree1_withOutgroup_Second$edge.length
plot(tree5_withOutgroup_Second,cex=0.05)

tree6_withOutgroup_Second$edge.length<-tree6_withOutgroup_Second$edge.length/tree1_withOutgroup_Second$edge.length
plot(tree5_withOutgroup_Second,cex=0.05)

#Second replicate with NO outgroup.class model H5 
Second_replicate_NOOutgroup_Phylo_H5<-read.tree("SecondDataSet_LocationsNOOutgroupH5GHOST.fasta.treefile")


tree1_NOOutgroup_Second<-Second_replicate_NOOutgroup_Phylo_H5[[1]]

tree2_NOOutgroup_Second<-Second_replicate_NOOutgroup_Phylo_H5[[2]]

tree3_NOOutgroup_Second<-Second_replicate_NOOutgroup_Phylo_H5[[3]]

tree4_NOOutgroup_Second<-Second_replicate_NOOutgroup_Phylo_H5[[4]]

tree5_NOOutgroup_Second<-Second_replicate_NOOutgroup_Phylo_H5[[5]]

tree6_NOOutgroup_Second<-Second_replicate_NOOutgroup_Phylo_H5[[6]]


tree2_NOOutgroup_Second$edge.length<-tree2_NOOutgroup_Second$edge.length/tree1_NOOutgroup_Second$edge.length
plot(tree2_NOOutgroup_Second,cex=0.05)

tree3_NOOutgroup_Second$edge.length<-tree3_NOOutgroup_Second$edge.length/tree1_NOOutgroup_Second$edge.length
plot(tree3_NOOutgroup_Second,cex=0.05)

tree4_NOOutgroup_Second$edge.length<-tree4_NOOutgroup_Second$edge.length/tree1_NOOutgroup_Second$edge.length
plot(tree4_NOOutgroup_Second,cex=0.05)

tree5_NOOutgroup_Second$edge.length<-tree5_NOOutgroup_Second$edge.length/tree1_NOOutgroup_Second$edge.length
plot(tree5_NOOutgroup_Second,cex=0.05)

tree6_NOOutgroup_Second$edge.length<-tree6_NOOutgroup_Second$edge.length/tree1_NOOutgroup_Second$edge.length
plot(tree5_NOOutgroup_Second,cex=0.05)


#Third replicate with outgroup. classmodel H5 

