#Third replicate NO outgroup. classmodel H4 

Third_replicate_NOOutgroup_Phylo_H4<-read.tree("ThirdDataSet_LocationsNOOutgroupH4GHOST.fasta.treefile")

tree1_NOOutgroup_Three<-Third_replicate_NOOutgroup_Phylo_H4[[1]]

