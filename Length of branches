#Fourth replicate with outgroup. classmodel H3

Fourth_replicate_withOutgroup_Phylo_H3<-read.tree("FourthDataSet_LocationswithOutgroupH3GHOST.fasta.treefile")

tree1_withOutgroup_Four<-Fourth_replicate_withOutgroup_Phylo_H3[[1]]

tree2_withOutgroup_Four<-Fourth_replicate_withOutgroup_Phylo_H3[[2]]

tree3_withOutgroup_Four<-Fourth_replicate_withOutgroup_Phylo_H3[[3]]

tree4_withOutgroup_Four<-Fourth_replicate_withOutgroup_Phylo_H3[[4]]

tree2_withOutgroup_Four$edge.length<-tree2_withOutgroup_Four$edge.length/tree1_withOutgroup_Four$edge.length
plot(tree2_withOutgroup_Four,cex=0.05)

tree3_withOutgroup_Four$edge.length<-tree3_withOutgroup_Four$edge.length/tree1_withOutgroup_Four$edge.length
plot(tree3_withOutgroup_Four,cex=0.05)

tree4_withOutgroup_Four$edge.length<-tree4_withOutgroup_Four$edge.length/tree1_withOutgroup_Four$edge.length
plot(tree4_withOutgroup_Four,cex=0.05)

#Fourth replicate with NO outgroup. classmodel H3

Fourth_replicate_NOOutgroup_Phylo_H3<-read.tree("FourthDataSet_LocationsNOOutgroupH3GHOST.fasta.treefile")

tree1_NOOutgroup_Four<-Fourth_replicate_NOOutgroup_Phylo_H3[[1]]

tree2_NOOutgroup_Four<-Fourth_replicate_NOOutgroup_Phylo_H3[[2]]

tree3_NOOutgroup_Four<-Fourth_replicate_NOOutgroup_Phylo_H3[[3]]

tree4_NOOutgroup_Four<-Fourth_replicate_NOOutgroup_Phylo_H3[[4]]

tree2_NOOutgroup_Four$edge.length<-tree2_NOOutgroup_Four$edge.length/tree1_NOOutgroup_Four$edge.length
tr2col <- rep("#000000", Nedge(tree2_NOOutgroup_Four))
tr2col[which(tree2_NOOutgroup_Four$edge.length > quantile(tree2_NOOutgroup_Four$edge.length, prob = 0.95))] <- "#35b779"
plot(tree2_NOOutgroup_Four,cex=0.05, edge.color = tr2col, edge.width = 3)

tree3_NOOutgroup_Four$edge.length<-tree3_NOOutgroup_Four$edge.length/tree1_NOOutgroup_Four$edge.length
tr3col <- rep("#000000", Nedge(tree3_NOOutgroup_Four))
tr3col[which(tree3_NOOutgroup_Four$edge.length > quantile(tree3_NOOutgroup_Four$edge.length, prob = 0.95))] <- "#31688e"
plot(tree3_NOOutgroup_Four,cex=0.05, edge.color = tr3col, edge.width = 3)

tree4_NOOutgroup_Four$edge.length<-tree4_NOOutgroup_Four$edge.length/tree1_NOOutgroup_Four$edge.length
tr4col <- rep("#000000", Nedge(tree4_NOOutgroup_Four))
tr4col[which(tree4_NOOutgroup_Four$edge.length > quantile(tree4_NOOutgroup_Four$edge.length, prob = 0.95))] <- "#fde725"
plot(tree4_NOOutgroup_Four,cex=0.05, edge.color = tr4col, edge.width = 3)

plot(tree1_NOOutgroup_Four,cex=0.05)

#Fifth replicate with outgroup. classmodel H3 
