tree4_NOOutgroup_Three<-Third_replicate_NOOutgroup_Phylo_H4[[4]]

tree5_NOOutgroup_Three<-Third_replicate_NOOutgroup_Phylo_H4[[5]]

tree2_NOOutgroup_Three$edge.length<-tree2_NOOutgroup_Three$edge.length/tree1_NOOutgroup_Three$edge.length
plot(tree2_NOOutgroup_Three,cex=0.05)

tree3_NOOutgroup_Three$edge.length<-tree3_NOOutgroup_Three$edge.length/tree1_NOOutgroup_Three$edge.length
plot(tree3_NOOutgroup_Three,cex=0.05)

tree4_NOOutgroup_Three$edge.length<-tree4_NOOutgroup_Three$edge.length/tree1_NOOutgroup_Three$edge.length
plot(tree4_NOOutgroup_Three,cex=0.05)

tree5_NOOutgroup_Three$edge.length<-tree5_NOOutgroup_Three$edge.length/tree1_NOOutgroup_Three$edge.length
